# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/config/database.yml
/config/newrelic.yml
/config/initializers/secret_token.rb
/log/*.log
/tmp
/.idea
/.rvmrc
/.ruby-version
/.directory
.DS_Store
/tags
/public/assets
/public/uploads
