<div class="row main">
  <div class="span3">
    <%= render "categories_map" %>
  </div>
  <div class="span9">
    <%= render "area_map" %>
    <div id="items-list">
      <% @categories.each do |c| %>
      <%= render_cell :items, :homepage_latest_items ,{:category_id => c.id, :categories => @categories} %>
      <% end %>
    </div>
  </div>
</div>
